{
  "name": "auth-tutorial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx scripts/server-init.ts && next dev",
    "build": "prisma generate && next build",
    "start": "tsx scripts/server-init.ts && next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prepare": "prisma generate",
    "db:init": "tsx scripts/init-db.ts",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "health": "tsx scripts/health-check.ts",
    "db:warmup": "tsx -e \"import { warmupDatabaseConnection } from './lib/startup'; warmupDatabaseConnection();\"",
    "db:check": "node scripts/check-db.js",
    "db:monitor": "node scripts/check-db.js && node -e \"setInterval(() => require('./scripts/check-db'), 60000)\"",
    "predeploy": "node scripts/deploy-hooks.js pre",
    "postdeploy": "node scripts/deploy-hooks.js post",
    "setup:r2": "node scripts/setup-r2.js",
    "cleanup:uploads": "node scripts/cleanup-local-uploads.js",
    "storage:migrate": "node scripts/setup-r2.js && node scripts/cleanup-local-uploads.js",
    "r2:update": "node scripts/update-r2-config.js",
    "prepare:deploy": "node scripts/prepare-deploy.js",
    "deploy:vercel": "npm run prepare:deploy && vercel --prod",
    "docker:build": "docker build -t lottery-app .",
    "docker:run": "docker run -p 3000:3000 lottery-app",
    "typecheck": "tsc --noEmit",
    "bun:dev": "bun run scripts/server-init.ts && bun --bun next dev",
    "bun:build": "bun prisma generate && bun --bun next build",
    "bun:start": "bun run scripts/server-init.ts && bun --bun next start"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/s3-request-presigner": "^3.525.0",
    "@hookform/resolvers": "^3.3.3",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.511.0",
    "nanoid": "^5.1.5",
    "next": ">=14.2.25",
    "next-auth": "^5.0.0-beta.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.12.0",
    "react-spinners": "^0.13.8",
    "readline": "^1.3.0",
    "resend": "^2.1.0",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.19.4",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "prisma"
  ]
}
